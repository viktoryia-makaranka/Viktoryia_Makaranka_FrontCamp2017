{
  "name": "front_camp_2017",
  "version": "0.0.1",
  "description": "Front Camp 2017 by Viktoryia Makaranka",
  "private": true,
  "scripts": {
    "build:dev": "npm run lint:js && webpack --config webpack/webpack.dev.config.js",
    "build:watch": "npm run lint:js && webpack --watch --config webpack/webpack.dev.config.js",
    "build:prod": "npm run lint:js && webpack --config webpack/webpack.prod.config.js -p",
    "start": "webpack-dev-server --open --config webpack/webpack.dev.config.js",
    "lint:style": "stylelint less/*.less",
    "lint:js": "eslint . --ext .js --max-warnings 0",
    "express:start": "nodemon ./server/web-server.js --exec babel-node --presets env"
  },
  "author": "Viktoryia Makaranka",
  "devDependencies": {
    "autoprefixer": "^7.2.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "element-dataset": "^2.2.6",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.9.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fetch-polyfill": "^0.8.2",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "postcss-loader": "^2.0.9",
    "postcss-sass": "^0.3.0",
    "style-loader": "^0.19.0",
    "stylelint": "^8.3.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "express": "^4.16.2",
    "http-status": "^1.0.1",
    "mongodb-core": "^3.0.2",
    "mongoose": "^5.0.3",
    "nodemon": "^1.14.11",
    "pug": "^2.0.0-rc.4",
    "winston": "^3.0.0-rc1"
  }
}
